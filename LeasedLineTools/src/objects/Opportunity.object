    <validationRules>
        <fullName>Closed_Won_Mandatory_Fields</fullName>
        <active>true</active>
        <description>Minimum Contract Term, Cancellation Notice Period, Payment Term and Sub Status need to be set  when Stage is Closed Won</description>
        <errorConditionFormula>AND( 
$Setup.Validation_Toggle__c.Is_Validation_On__c,
ISPICKVAL(StageName, &apos;Closed Won&apos;),
OR(
ISBLANK(Cancellation_Notice_Period__c),
